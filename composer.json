{
  "name": "daalder/skeleton-package",
  "description": "Daalder skeleton package",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-zip": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0|^6.0",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Daalder\\SkeletonPackage\\Package\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Daalder\\SkeletonPackage\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
